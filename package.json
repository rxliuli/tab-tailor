{
  "name": "tab-tailor",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "pack-clean": "rimraf ./release && mkdir release",
    "pack-chrome": "pnpm build && pnpm pack-clean && jszip-cli add dist/ -o ./release/tab-tailor-chrome.zip",
    "docs-deploy": "gh-pages -d dist --dotfiles",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.16",
    "@ffflorian/jszip-cli": "^3.4.0",
    "@liuli-util/prettier-standard-config": "^0.3.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.0"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "idb-keyval": "^6.2.0",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-use": "^17.4.0"
  }
}
